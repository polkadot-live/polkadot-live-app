{
  "name": "polkadot-live-vite",
  "productName": "Polkadot Live",
  "version": "0.3.0-alpha",
  "description": "Decentralised Polkadot Notifications and Chain Interactions on the Desktop.",
  "main": "dist/main/main.cjs",
  "type": "module",
  "contributors": [
    "Dane Bulat<dane.bulat@gmail.com>"
  ],
  "scripts": {
    "clear": "rm -rf node_modules out tsconfig.tsbuildinfo",
    "lint": "eslint --fix --ext .ts,.tsx . && npx prettier --write ./src",
    "wdio": "NODE_ENV=test wdio run wdio.conf.ts",
    "test": "mocha --require ts-node/register --config .mocharc.jsonc test/unit/**/*.test.ts",
    "vendors": "cross-env ELECTRON_RUN_AS_NODE=1 electron scripts/update-electron-vendors.js",
    "watch": "cross-env DEBUG=1 MODE=development node scripts/watch.js",
    "build:renderer": "vite build --config config/vite.renderer.config.js",
    "build:preload": "vite build --config config/vite.preload.config.js",
    "build:main": "vite build --config config/vite.main.config.js",
    "build:all": "concurrently \"yarn run build:renderer\" \"yarn run build:preload\" \"yarn run build:main\"",
    "package": "cross-env MODE=production yarn run build:all && electron-builder --config scripts/electron-builder.config.prod.mjs",
    "package:dev": "cross-env MODE=development yarn run build:all && electron-builder --config scripts/electron-builder.config.dev.mjs",
    "package:win": "cross-env MODE=production CSC_IDENTITY_AUTO_DISCOVERY=false yarn run build:all && electron-builder build --win --config scripts/electron-builder.config.prod.mjs",
    "asar:list": "asar list"
  },
  "engines": {
    "node": ">=23.1.0"
  },
  "keywords": [],
  "author": {
    "name": "Dane Bulat",
    "email": "dane.bulat@gmail.com"
  },
  "license": "GPL-3.0-only",
