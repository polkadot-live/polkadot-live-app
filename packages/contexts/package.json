{
  "name": "@polkadot-live/contexts",
  "version": "0.12.0-beta",
  "type": "module",
  "private": true,
  "exports": {
    ".": "./src/index.ts",
    "./types/action": "./src/types/action/index.ts",
    "./types/common": "./src/types/common/index.ts",
    "./types/import": "./src/types/import/index.ts",
    "./types/main": "./src/types/main/index.ts",
    "./types/openGov": "./src/types/openGov/index.ts",
    "./types/settings": "./src/types/settings/index.ts",
    "./types/tabs": "./src/types/tabs/index.ts"
  },
  "scripts": {
    "build": "cross-env MODE=production vite build --config vite.config.js",
    "build:dev": "cross-env MODE=development vite build --config vite.config.js",
    "clear:dist": "[ -d \"node_modules\" ] && rm -fr \"node_modules\" || true",
    "clear": "([ -d \"dist\" ] && rm -fr \"dist\") || true && ([ -d \"node_modules\" ] && rm -fr \"node_modules\") || true",
    "lint": "eslint --fix ./src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@polkadot-live/core": "workspace:*",
    "@polkadot-live/styles": "workspace:*"
  },
  "devDependencies": {
    "@polkadot-live/types": "workspace:*",
    "@types/wicg-file-system-access": "^2023.10.7"
  },
  "packageManager": "yarn@4.0.2"
}
