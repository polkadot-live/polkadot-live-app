{
  "name": "@polkadot-live/core",
  "version": "0.8.0-alpha",
  "type": "module",
  "private": true,
  "exports": {
    "./accounts/*": "./src/accounts/*.ts",
    "./config/*": "./src/config/*.ts",
    "./controllers": "./src/controllers/index.ts",
    "./intervaled": "./src/callbacks/intervaled.ts",
    "./lib/accounts": "./src/library/AccountsLib/index.ts",
    "./lib/api": "./src/library/ApiLib.ts",
    "./lib/callbacks": "./src/library/CallbacksLib.ts",
    "./lib/common": "./src/library/CommonLib.ts",
    "./lib/events": "./src/library/EventsLib.ts",
    "./lib/import": "./src/library/ImportLib.ts",
    "./lib/openGov": "./src/library/OpenGovLib.ts",
    "./lib/text": "./src/library/TextLib.ts",
    "./lib/time": "./src/library/TimeLib.ts",
    "./model": "./src/model/index.ts",
    "./oneshots": "./src/callbacks/oneshots.ts",
    "./orchestrators": "./src/orchestrators/index.ts"
  },
  "scripts": {
    "lint": "eslint --fix --ext .ts,.tsx . && npx prettier --write ./src",
    "clear:dist": "([ -d \"dist\" ] && rm -fr \"dist\") || true && ([ -d \"node_modules\" ] && rm -fr \"node_modules\") || true",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@polkadot-live/consts": "workspace:*",
    "@polkadot-live/types": "workspace:*"
  },
  "packageManager": "yarn@4.0.2"
}
