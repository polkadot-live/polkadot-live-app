{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "checkJs": false,
    "strict": true,
    "lib": ["DOM", "DOM.Iterable", "ESNext"],
    "sourceMap": false,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,

    "declaration": true,
    "declarationDir": ".",
    //"emitDeclarationOnly": true,
  },
  "include": ["./**/*.ts"],
}