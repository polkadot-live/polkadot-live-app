{
  "name": "@polkadot-live/ui",
  "version": "0.3.0-alpha",
  "type": "module",
  "private": true,
  "files": [
    "dist/**"
  ],
  "exports": {
    "./components": "./dist/mjs/components/index.js",
    "./components/help": "./dist/mjs/components/Help/index.js",
    "./components/qrcode": "./dist/mjs/components/QRCode/index.js",
